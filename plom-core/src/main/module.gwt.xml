<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.8.2//EN"
  "https://raw.githubusercontent.com/gwtproject/gwt/master/distro-source/core/src/gwt-module.dtd">
<module rename-to='plomcore'>
  <inherits name='elemental.Elemental'/>
  <inherits name="jsinterop.base.Base" />
  
  <entry-point class='org.programmingbasics.plom.core.Entry'/>

  <source path='core'/>
  <super-source path='super'/>

  <define-linker name="scripttag" class="org.programmingbasics.plom.gwtlinker.ScriptTagLinker" />
  <!-- 
  When debugging on Edge, it's easier to import the JS directly in a
  script tag instead of the xsiframe.
  -->
  <add-linker name="scripttag"/>

  <!-- <add-linker name="sso"/> -->
  <!-- <add-linker name="xsiframe"/>  -->
</module>
